<?xml version="1.0" encoding="UTF-8"?>
 <!-- note tree -->      
    <qx:widget
      id="folderTreePanel" 
      qxt:class="collapsablepanel.Panel"
      caption="Notes"
      appearance="collapsable-panel-classic"
      qxt:flex="1" 
      value="false"
      padding="0">
      
      <bibcoll:noteTree 
        widgetId="mainNoteTree"
        serviceName="bibliograph.controller.Note"
        columnHeaders="{js}['Notes','Owner']"
        width="200">
        <qxt:observe 
          propertyChain="datasource"
          source="this"
          sourcePropertyChain="datasource" />
        <qxt:bind
          propertyChain="nodeId"
          target="this"
          targetPropertyChain="noteId"/>
        <qxt:observe 
          propertyChain="nodeId"
          source="this"
          sourcePropertyChain="noteId" />                  
       </bibcoll:noteTree>
            </qx:widget>