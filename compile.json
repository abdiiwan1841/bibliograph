{
  "eslintConfig": {
    "globals": {
      "JSZip": false
    },
    "parserOptions": {
      "ecmaVersion": 2017
    }
  },
  "libraries": [
    "src/client/bibliograph",
    "src/server/modules/z3950/client",
    "src/server/modules/webservices/client",
    "src/server/modules/backup/client",
    "src/lib/incubator.qx.io.jsonrpc"
  ],
  "environment": {
    "qx.icontheme": "Tango",
    "module.objectid": true,
    "app.version": "3.0.0-beta.79",
    "app.serverUrl": "../../../src/server/test"
  },
  "locales": [
    "en",
    "de"
  ],
  "defaultTarget": "source",
  "targets": [
    {
      "description": "The local 'source' target for development",
      "type": "source",
      "environment": {
        "app.mode": "development",
        "qx.debug": true
      },
      "bundle": {
        "include": [
          "qx.*",
          "qxl.*",
          "qookery.*"
        ],
        "exclude": [
          "qx.util.*"
        ]
      },
      "outputPath": "compiled/source"
    },
    {
      "description": "The local 'build' target ",
      "type": "build",
      "environment": {
        "app.mode": "production",
        "qx.debug": false
      },
      "outputPath": "compiled/build"
    },
    {
      "description": "A source build to be deployed for debugging purposes",
      "type": "dist-debug",
      "environment": {
        "app.mode": "development",
        "app.serverUrl": "server.php?r="
      },
      "outputPath": "compiled/dist-debug",
      "targetClass": "SourceTarget",
      "bundle": {
        "include": [
          "qx.*",
          "qxl.*",
          "qookery.*"
        ],
        "exclude": [
          "qx.util.*"
        ]
      }
    }
  ],
  "applications": [
    {
      "title": "Bibliograph Bibliographic Data Manager",
      "name": "bibliograph",
      "theme": "bibliograph.theme.Theme",
      "class": "bibliograph.Application",
      "bootPath": "source/boot",
      "include": [
        "bibliograph.plugins.*"
      ],
      "environment": {}
    },
    {
      "title": "Qooxdoo API Viewer",
      "class": "qxl.apiviewer.Application",
      "theme": "qxl.apiviewer.Theme",
      "name": "apiviewer",
      "environment": {
        "qx.icontheme": "Tango",
        "excludeFromAPIViewer": [
          "qx.*",
          "qxl.apiviewer.*"
        ]
      },
      "exclude": [
        "qx.test.*",
        "qx.module.Blocker",
        "qx.module.Placement"
      ]
    },
    {
      "class": "qxl.testtapper.Application",
      "name": "testtapper",
      "theme": "qx.theme.Simple",
      "title": "Qooxdoo TestTAPper",
      "environment": {
        "qx.icontheme": "Tango",
        "testtapper.testNameSpace": "bibliograph.test"
      },
      "include": [
        "bibliograph.test.*"
      ],
      "exclude": []
    }
  ],
  "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json"
}
