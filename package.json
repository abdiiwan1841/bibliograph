{
  "name": "bibliograph",
  "version": "3.0.0-beta.79",
  "description": "Bibliograph is a powerful open source web application for the collaborative collection, editing and publishing of bibliographic data.",
  "main": "",
  "directories": {
    "test": "test"
  },
  "homepage": "http://www.bibliograph.org",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/caolan/async/raw/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cboulanger/bibliograph.git"
  },
  "scripts": {
    "postinstall": "build/script/update-dependencies",
    "gui": "screwy",
    "test": "build/script/start-mysql && test/bin/unit-functional && test/script/api || exit $?",
    "Start Development Server": "build/script/start",
    "Start GUI Test Recorder": "build/script/start source bibliograph-recorder",
    "Start GUI Test Player": "build/script/start source bibliograph-player",
    "Start CLI Tests": "test",
    "Start CLI Tests (Extended)": "test/bin/unit-functional && test/script/api development || exit $?",
    "Continuous Compilation": "build/script/start server-only",
    "View App Log": "build/script/log-view",
    "View Error log": "build/script/log-view error",
    "Clear logs": "build/script/log-clear && build/script/log-clear error",
    "Create new migration": "build/script/run-in-new-terminal $(pwd)/build/script/create-migration",
    "Create JSONRPC proxy methods": "cd src/server && php yii rpc-proxy/create",
    "Extract translations": "cd src/server && php yii message config/i18n.php",
    "Compile modules": "build/script/modules-compile",
    "Build distributable package (PHP 7.3)": "build/script/dist-build || exit $?",
    "Build distributable package (PHP 7.0)": "build/script/dist-build-php70 || exit $?",
    "Build debuggable package (PHP 7.3)": "build/script/dist-build dist-debug || exit $?",
    "Build debuggable package (PHP 7.0)": "build/script/dist-build-php70 dist-debug || exit $?",
    "Run production build": "build/script/dist-run",
    "Clean up production build": "build/script/dist-clean",
    "Release on GitHub": "build/script/dist-clean && npx release-it --preRelease=beta || exit $?",
    "Release on GitHub (Clean build)": "node build/script/github-release-clean.js && build/script/github-tag-clean || exit $?",
    "Build docker image": "build/script/docker-build",
    "Run docker container": "build/script/docker-run",
    "Create & start docker container": "npm run dist-create && npm run docker-build && npm run docker-start || exit $?",
    "Clean up docker": "docker container prune && docker image prune"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@qooxdoo/compiler": "^1.0.0-beta.20200111-0015",
    "@qooxdoo/eslint-config-jsdoc-disable": "^1.0.2",
    "@qooxdoo/eslint-config-qx": "^1.3.5",
    "raptor-client": "github:cboulanger/raptor-client",
    "webpack-cli": "^3.3.10"
  },
  "devDependencies": {
    "@qooxdoo/eslint-plugin-qx": "^1.2.1",
    "array-equal": "^1.0.0",
    "dotenv": "^8.0.0",
    "eslint": "^6.1.0",
    "eslint-config-qx": "^0.2.6",
    "eslint-plugin-import": "^2.18.2",
    "eventsource": "^1.0.7",
    "git-hooks": "^1.1.10",
    "github-remove-all-releases": "^1.0.1",
    "json-diff": "^0.5.4",
    "mocha": "^6.2.0",
    "r2": "^2.0.1",
    "release-it": "^12.3.5",
    "run-script-os": "^1.0.7",
    "screwy": "^1.0.5",
    "standard": "^13.1.0",
    "webpack": "^4.39.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "qx": true,
      "qcl": true,
      "dialog": true,
      "bibliograph": true
    }
  }
}
