{
  "name": "bibliograph",
  "version": "3.0.0-dev",
  "description": "Bibliograph is a powerful open source web application for the collaborative collection, editing and publishing of bibliographic data.",
  "main": "",
  "directories": {
    "test": "test"
  },
  "homepage": "http://www.bibliograph.org",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/caolan/async/raw/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cboulanger/bibliograph.git"
  },
  "scripts": {
    "serve-dev": "run-script-os",
    "serve-dev:darwin:linux": "bash bin/source-server.sh",
    "serve-dev:win32": "echo 'Sorry, server can be run on MacOs/Linux platforms only.'",
    "test": "run-script-os",
    "test:darwin:linux": "bash test/codeception-all.sh && bash test/api.sh",
    "test:win32": "echo 'Sorry, tests can be run on MacOs/Linux platforms only.'",
    "test-dev": "run-script-os",
    "test-dev:darwin:linux": "bash test/codeception-all.sh && bash test/api.sh development",
    "test-dev:win32": "echo 'Sorry, tests can be run on MacOs/Linux platforms only.'",
    "codeception": "bash test/setup.sh && bash test/codeception.sh"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "qooxdoo": "^6.0.1",
    "qooxdoo-sdk": "^6.0.0-alpha",
    "qxcompiler": "^0.2.0",
    "raptor-client": "github:cboulanger/raptor-client",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "array-equal": "^1.0.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eventsource": "^1.0.5",
    "json-diff": "^0.5.2",
    "mocha": "^4.1.0",
    "r2": "^2.0.0",
    "run-script-os": "^1.0.3"
  }
}
