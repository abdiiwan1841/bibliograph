#!/usr/bin/env bash

if hash mysql.server 2>/dev/null; then
  # assume we have mysql from homebrew

  if [[ "$(mysql.server status)" =~ "running" ]]; then
    echo "MySQL server is running...";
  else
    mysql.server start
  fi;
  exit 0
else
  echo "Mysql server support only implemented for homebrew on os x."
  exit 1
fi
